ec146a71f5f9d55fcc9c962cb5c1646b|file:///Users/sophieyou/zsh_widgets.zsh|1|1|v|zsh
	
	_fuzzybrowse_zsh_insert_output() {
	  local args="$@"
	  local res
	  () {
	    fuzzybrowse -o "$1" "$args"
	    res="$(<$1 tr "\\n" " ")"
	  } =(:)
	  zle reset-prompt
	  if [[ -n "$res" ]]; then
	    res="$res[0,-2]"
	    LBUFFER+=" $res"
	    if [[ -d "$res" ]]; then
	      LBUFFER+="/"
	    else
	      LBUFFER+=" "
	    fi
	  fi
	}
	
	zle -N _fuzzybrowse_zsh_insert_output
